import java.util.*;
public class Tugas_Kelompok {
  public static void main (String [] warteg){
  Scanner sc=new Scanner(System.in);
	
	String []os;
    int rim=0;
    int ios=0;
    int android=0;
    int windows=0;
    String simpan="RIM";
      os=new String[10];
	  
	  System.out.println("pilih 1.RIM     :");
	  System.out.println("pilih 2.ios     :");
	  System.out.println("pilih 3.Android :");
	  System.out.println("pilih 4.Windows :");
	  System.out.println("");
    
      for (int i=0;i<os.length;i++){
      System.out.print("Masukan OS Mobile:");
      os[i]=sc.next();
      if (os[i].equalsIgnoreCase("1")){
        rim+=1;}
      else
      {if(os[i].equalsIgnoreCase("2")){
        ios+=1;}
      else{if(os[i].equalsIgnoreCase("3")){
      android+=1;}
      else if (os[i].equalsIgnoreCase("4")){
       windows+=1;}
      else {System.exit(0);}
      }
      }
      }
      System.out.println("Pengguna OS RIM Sebanyak : "+rim);
      System.out.println("Pengguna OS IOS Sebanyak : "+ios);
      System.out.println("Pengguna OS Android Sebanyak : "+android);
      System.out.println("Pengguna OS Windows Phone Sebanyak : "+windows);
	  
    int total;
    
    if (rim>ios){
      total=rim;
      System.out.println("Pengguna Terbanyak : " +total+ "rim");}
    
     else if (ios>rim){
      total=ios;
      System.out.println("Pengguna Terbanyak : " +total+"ios");}
    
    else if (android>ios){
    total=android;
    System.out.println("Pengguna Terbanyak : " +total +"Android");}
    
    else if (ios>android){
      total=ios;
      System.out.println("Pengguna Terbanyak : " +total+ "Ios");}
    
    else if (android>windows){
      total=android;
      System.out.println("Pengguna Terbanyak : " +total +"Android");}  
    
    else if (windows>android){
      total=windows;
      System.out.println("Pengguna Terbanyak : " +total +"Adalah Windows");} 
 
} 

}

